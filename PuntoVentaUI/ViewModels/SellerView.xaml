<UserControl
  x:Class="PuntoVentaUI.ViewModels.SellerView"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:local="clr-namespace:PuntoVentaUI.ViewModels"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  d:DesignHeight="273"
  d:DesignWidth="204.199"
  Background="#FFDADADA"
  mc:Ignorable="d">
  <Grid>
    <TextBlock
      Margin="34,36,0,0"
      HorizontalAlignment="Left"
      VerticalAlignment="Top"
      FontSize="16"
      Foreground="{DynamicResource Text-Level1-Foreground}"
      Text="Beny Linus"
      TextWrapping="Wrap" />
    <Image
      Width="154"
      Height="156"
      Margin="24,62,0,0"
      HorizontalAlignment="Left"
      VerticalAlignment="Top"
      Source="/PuntoVentaUI;component/Imagenes/fotousuario.png" />
    <Button
      Width="89"
      Height="40"
      Margin="59,223,0,0"
      HorizontalAlignment="Left"
      VerticalAlignment="Top"
      Content="Refresh" />
    <TextBlock
      Margin="34,10,0,0"
      HorizontalAlignment="Left"
      VerticalAlignment="Top"
      FontSize="14"
      FontWeight="Bold"
      Foreground="{DynamicResource Principal-Foreground}"
      Text="Vendedor"
      TextWrapping="Wrap" />
  </Grid>
</UserControl>
